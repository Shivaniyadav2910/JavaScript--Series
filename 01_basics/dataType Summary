const id =Symbol('123');
const anotherId=Symbol('123');

console.log(id===anotherId);  //false

const bigNumber = 3456543576654356754n;   //bigint

//Reference (Non primitive)
//Array,objects,Function

const heros=["shaktiman","naagraj","doga"];
